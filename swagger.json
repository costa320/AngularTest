{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Node js open-table CRUD",
    "description": "My crawler",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "host": "localhost:8000",
  "basePath": "/",

  "schemes": ["http"],
  "consumes": ["application/json"],
  "produces": ["application/json"],

  "paths": {
    "/api/restaurant/getRestaurants/{skip}/{take}": {
      "parameters": [
        {
          "name": "skip",
          "in": "path",
          "required": true,
          "description": "Skip",
          "type": "number"
        },
        {
          "name": "take",
          "in": "path",
          "required": true,
          "description": "Skip",
          "type": "number"
        }
      ],
      "get": {
        "tags": ["Restaurants"],
        "summary": "Get a list of restaurants.",
        "responses": {
          "200": {
            "description": "200 response",
            "schema": { "$ref": "#/definitions/arrRestaurants" }
          }
        }
      }
    },
    "/api/tipologiche/getCuisinesList": {
      "parameters": [],
      "get": {
        "tags": ["Tipologiche"],
        "summary": "Get a list of cuisines.",
        "responses": {
          "200": {
            "description": "200 response",
            "schema": { "$ref": "#/definitions/arrCousins" }
          }
        }
      }
    }
  },
  "definitions": {
    "arrRestaurants": {
      "required": ["arrayList"],
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "92fb9513-73ad-5de9-a45c-769f33befcb8"
          },
          "restaurantName": {
            "type": "string",
            "example": "Pots and Ladles"
          },
          "companyName": {
            "type": "string",
            "example": "Zions Bancorporation"
          },
          "cuisine": {
            "type": "array",
            "items": {
              "type": "string",
              "example": "British Virgin Islands"
            }
          },
          "phone": {
            "type": "string",
            "example": "+48 91 707 98 15"
          },
          "email": {
            "type": "string",
            "example": "badon@ZionsBancorporation.com"
          },
          "geolocation": {
            "type": "object",
            "properties": {
              "country": {
                "type": "string",
                "example": "Bermuda"
              },
              "city": {
                "type": "string",
                "example": "Sioux Falls"
              },
              "address": {
                "type": "string",
                "example": "63843 Leora Circles Apt. 112"
              }
            }
          },
          "sideProducts": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "Table"
                },
                "material": {
                  "type": "string",
                  "example": "Wooden"
                },
                "description": {
                  "type": "string",
                  "example": "New ABC 13 9370, 13.3, 5th Gen CoreA5-8250U, 8GB RAM, 256GB SSD, power UHD Graphics, OS 10 Home, OS Office A & J 2016"
                },
                "adjectives": {
                  "type": "string",
                  "example": "Luxurious"
                },
                "color": {
                  "type": "string",
                  "example": "purple"
                }
              }
            }
          },
          "priceRange": {
            "type": "array",
            "items": {
              "type": "string",
              "example": ["$83.00", "$158"]
            }
          }
        }
      }
    },
    "arrCousins": {
      "required": ["arrayList"],
      "type": "array",
      "items": {
        "type": "string",
        "example": "British Virgin Islands"
      }
    }
  }
}
