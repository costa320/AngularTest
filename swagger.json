{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Node js open-table CRUD",
    "description": "My crawler",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "host": "localhost:8000",
  "basePath": "/",

  "schemes": ["http"],
  "consumes": ["application/json"],
  "produces": ["application/json"],

  "paths": {
    "/api/restaurant/getRestaurants/{skip}/{take}": {
      "parameters": [
         {
          "name": "skip",
          "in": "path",
          "required": true,
          "description": "Skip",
          "type": "number"
        },
         {
          "name": "take",
          "in": "path",
          "required": true,
          "description": "Skip",
          "type": "number"
        }
      ],
      "get": {
        "tags": ["Restaurants"],
        "summary": "Get a list of restaurants.",
        "responses": {
          "200": {
            "description": "200 response",
            "schema": { "$ref": "#/definitions/arrRestaurants" }
          }
        }
      }
    },
    "/api/tipologiche/getCuisinesList": {
      "parameters": [],
      "get": {
        "tags": ["Tipologiche"],
        "summary": "Get a list of cuisines.",
        "responses": {
          "200": {
            "description": "200 response",
            "schema": { "$ref": "#/definitions/arrCousins" }
          }
        }
      }
    }
  },
  "definitions": {
    "arrRestaurants": {
      "required": ["arrayList"],
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "92fb9513-73ad-5de9-a45c-769f33befcb8"
          },
          "restaurantName": {
            "type": "string",
            "example": "Pots and Ladles"
          },
          "companyName": {
            "type": "string",
            "example": "Zions Bancorporation"
          },
          "cuisine": {
           "type": "array",
           "items": {
            "type": "string",
            "example": "British Virgin Islands"
           }
          },
          "phone": {
            "type": "string",
            "example": "+48 91 707 98 15"
          },
          "email": {
            "type": "string",
            "example": "badon@ZionsBancorporation.com"
          }
        }
      }
    },
    "arrCousins": {
      "required": ["arrayList"],
      "type": "array",
      "items": {
       "type": "string",
       "example": "British Virgin Islands"
      }
    }
  }
}
